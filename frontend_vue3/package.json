    "@mdi/font": "5.9.55",
    "pinia": "^2.0.14",
    "roboto-fontface": "*",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.36",
